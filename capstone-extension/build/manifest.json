{
  "manifest_version": 3,
  "name": "Capstone Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest"
],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
  
}
